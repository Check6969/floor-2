    firesignal(game:GetService("ReplicatedStorage").EntityInfo.Caption.OnClientEvent, 'You already executed this mod, Returning',true,1)

    return false

end

local Test = Instance.new("Part")

Test.Name = "Endless"

Test.Parent = game:GetService("ReplicatedStorage")

Test = 1
