    end
end)()

-- Rebound Mimic
coroutine.wrap(function()
    while true do
        wait(550)
game.ReplicatedStorage.GameData.LatestRoom.Changed:Wait()
workspace.Mimic.BoundMimic1:Destroy()
wait(1.55)
if workspace:FindFirstChild("Depth") == nil and workspace:FindFirstChild("RushMoving") == nil and workspace:FindFirstChild("Bound") == nil and workspace:FindFirstChild("AmbushMoving") == nil and game:GetService("ReplicatedStorage").GameData.LatestRoom.Value ~= 50 then
loadstring(game:HttpGet("https://raw.githubusercontent.com/ThatNoah/entites/main/Boundmimic.txt"))()
end
    end
end)()

